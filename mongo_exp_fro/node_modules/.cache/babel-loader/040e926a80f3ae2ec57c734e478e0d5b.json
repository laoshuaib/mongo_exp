{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"addCuisine\",\n\n  data() {\n    return {\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {},\n      //某行数据\n      search: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    find() {\n      request.get(\"/ingredient/find\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    confirm() {\n      this.$confirm(\"若食材名被修改,相关配方等内容可能发生变动, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.update();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消修改\"\n        });\n      });\n    },\n\n    save() {\n      request.post(\"/ingredient/save\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.addDialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    load() {\n      request.get(\"/ingredient/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,YADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,CADR;MAELC,QAAQ,EAAE,EAFL;MAGLC,KAAK,EAAE,CAHF;MAILC,SAAS,EAAE,EAJN;MAKLC,IAAI,EAAE,EALD;MAKK;MACVC,MAAM,EAAE;IANH,CAAP;EAQD,CAZY;;EAabC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAfY;;EAgBbC,OAAO,EAAE;IACPC,IAAI,GAAG;MACLZ,OAAM,CACHa,GADH,CACO,kBADP,EAC2B;QACvBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKZ,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNI,MAAM,EAAE,KAAKA;QAHP;MADe,CAD3B,EAQGQ,IARH,CAQSC,GAAD,IAAS;QACb,KAAKX,SAAL,GAAiBW,GAAG,CAACf,IAAJ,CAASgB,OAA1B;QACA,KAAKb,KAAL,GAAaY,GAAG,CAACf,IAAJ,CAASG,KAAtB;MACD,CAXH;IAYD,CAdM;;IAePc,OAAO,GAAG;MACR,KAAKC,QAAL,CACE,8BADF,EAEE,IAFF,EAGE;QACEC,iBAAiB,EAAE,IADrB;QAEEC,gBAAgB,EAAE,IAFpB;QAGEC,IAAI,EAAE;MAHR,CAHF,EASGP,IATH,CASQ,MAAM;QACV,KAAKQ,MAAL;MACD,CAXH,EAYGC,KAZH,CAYS,MAAM;QACX,KAAKC,QAAL,CAAc;UACZH,IAAI,EAAE,MADM;UAEZI,OAAO,EAAE;QAFG,CAAd;MAID,CAjBH;IAkBD,CAlCM;;IAmCPC,IAAI,GAAG;MACL5B,OAAO,CAAC6B,IAAR,CAAa,kBAAb,EAAiC,KAAKtB,IAAtC,EAA4CS,IAA5C,CAAkDC,GAAD,IAAS;QACxD,IAAIA,GAAG,CAACa,IAAJ,KAAa,CAAjB,EAAoB;UAClB,KAAKJ,QAAL,CAAc;YACZH,IAAI,EAAE,SADM;YAEZI,OAAO,EAAE;UAFG,CAAd;UAIA,KAAKI,gBAAL,GAAwB,KAAxB;UACA,KAAKrB,IAAL;QACF,CAPA,MAOO;UACL,KAAKgB,QAAL,CAAc;YACZH,IAAI,EAAE,OADM;YAEZI,OAAO,EAAEV,GAAG,CAACe;UAFD,CAAd;QAIF;MACD,CAdD;IAeD,CAnDM;;IAoDPtB,IAAI,GAAG;MACLV,OAAM,CACHa,GADH,CACO,kBADP,EAC2B;QACvBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKZ,WADR;UAENC,QAAQ,EAAE,KAAKA;QAFT;MADe,CAD3B,EAOGY,IAPH,CAOSC,GAAD,IAAS;QACb,KAAKX,SAAL,GAAiBW,GAAG,CAACf,IAAJ,CAASgB,OAA1B;QACA,KAAKb,KAAL,GAAaY,GAAG,CAACf,IAAJ,CAASG,KAAtB;MACD,CAVH;IAWD,CAhEM;;IAiEP4B,UAAU,CAACC,GAAD,EAAM;MACd,KAAK3B,IAAL,GAAY4B,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAZ;MACA,KAAKI,aAAL,GAAqB,IAArB;IACD,CApEM;;IAqEPC,gBAAgB,CAACnC,QAAD,EAAW;MACzB;MACA,KAAKA,QAAL,GAAgBA,QAAhB;MACA,KAAKM,IAAL;IACD,CAzEM;;IA0EP8B,mBAAmB,CAACzB,OAAD,EAAU;M
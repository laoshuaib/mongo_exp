{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"feeRecord\",\n\n  data() {\n    return {\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {},\n      //用于新增费用\n      addDialogVisible: false,\n      uploadDialogVisible: false,\n      feeSearch: \"\",\n      userSearch: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    addConfirm() {\n      this.$confirm(\"费用一旦产生无法修改, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.save();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消新增\"\n        });\n      });\n    },\n\n    save() {\n      //设置处理人ID\n      let str = sessionStorage.getItem(\"user\");\n      this.form.userID = JSON.parse(str).id;\n      request.post(\"/fee/save\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.addDialogVisible = false;\n          this.form = {}; //置空\n\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    unionFind() {\n      request.get(\"/fee/unionFind\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          feeSearch: this.feeSearch,\n          userSearch: this.userSearch\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    findByFee() {\n      request.get(\"/fee/findByFee\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.feeSearch\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    findByUser() {\n      request.get(\"/fee/findByUser\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.userSearch\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    load() {\n      request.get(\"/fee/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0IA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,CADR;MAELC,QAAQ,EAAE,EAFL;MAGLC,KAAK,EAAE,CAHF;MAILC,SAAS,EAAE,EAJN;MAKLC,IAAI,EAAE,EALD;MAKK;MACVC,gBAAgB,EAAE,KANb;MAOLC,mBAAmB,EAAE,KAPhB;MAQLC,SAAS,EAAE,EARN;MASLC,UAAU,EAAE;IATP,CAAP;EAWD,CAfY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKC,QAAL,CAAc,mBAAd,EAAmC,IAAnC,EAAyC;QACvCC,iBAAiB,EAAE,IADoB;QAEvCC,gBAAgB,EAAE,IAFqB;QAGvCC,IAAI,EAAE;MAHiC,CAAzC,EAKGC,IALH,CAKQ,MAAM;QACV,KAAKC,IAAL;MACD,CAPH,EAQGC,KARH,CAQS,MAAM;QACX,KAAKC,QAAL,CAAc;UACZJ,IAAI,EAAE,MADM;UAEZK,OAAO,EAAE;QAFG,CAAd;MAID,CAbH;IAcD,CAhBM;;IAiBPH,IAAI,GAAG;MACL;MACA,IAAII,GAAE,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,CAAV;MACA,KAAKpB,IAAL,CAAUqB,MAAV,GAAmBC,IAAI,CAACC,KAAL,CAAWL,GAAX,EAAgBM,EAAnC;MACA/B,OAAO,CAACgC,IAAR,CAAa,WAAb,EAA0B,KAAKzB,IAA/B,EAAqCa,IAArC,CAA2Ca,GAAD,IAAS;QACjD,IAAIA,GAAG,CAACC,IAAJ,KAAa,CAAjB,EAAoB;UAClB,KAAKX,QAAL,CAAc;YACZJ,IAAI,EAAE,SADM;YAEZK,OAAO,EAAE;UAFG,CAAd;UAIA,KAAKhB,gBAAL,GAAwB,KAAxB;UACA,KAA
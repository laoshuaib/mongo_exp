 label=\"食材ID\">\n          <el-input v-model=\"form.id\" disabled></el-input>\n        </el-form-item>\n        <el-form-item label=\"食材名\" prop=\"change_name\">\n          <el-input v-model=\"form.name\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"储量(kg)\" prop=\"change_reserves\">\n          <el-input v-model=\"form.reserves\"></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n        <span>\n          <el-button type=\"primary\" size=\"default\" color=\"#fab6b6\" @click=\"save\"\n            >保存</el-button\n          >\n        </span>\n      </template>\n    </el-dialog>\n\n    <el-table :data=\"tableData\" border=\"border\">\n      <el-table-column prop=\"id\" label=\"食材ID\" width=\"120px\" />\n      <el-table-column prop=\"name\" label=\"食材名\" />\n      <el-table-column prop=\"reserves\" label=\"储量（单位kg）\" />\n      <el-table-column label=\"操作\" width=\"100px\" align=\"center\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\" color=\"#fab6b6\"\n            >编辑</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n        v-model:currentPage=\"currentPage\"\n        :page-sizes=\"[5, 10, 20]\"\n        :page-size=\"pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"../../untils/request\";\n\nexport default {\n  name: \"ingredientManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {}, //某行数据\n      rules: {\n        change_reserves: [\n          {\n            pattern: /^[1-9]\\d*\\.\\d*|0\\.\\d*[1-9]\\d*|0?\\.0+|0$/,\n            require: true,\n            message: \"不能为负数\",\n            trigger: \"blur\",\n          },\n        ],\n      },\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    save() {\n      request.post(\"/\", this.form).then((res) => {\n        console.log(res);\n        if (res.code === \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\",\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg,\n          });\n        }\n      });\n    },\n    load() {\n      request\n        .get(\"/ingredient/view\", {\n          params: {\n            pageNum: this.currentPage,\n            pageSize: this.pageSize,\n          },\n        })\n        .then((res) => {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        });\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n/deep/ .el-input {\n  width: 85%;\n}\n\n/deep/ .el-dialog {\n  background: #fcf9f4;\n}\n\n/deep/.el-form-item {\n  margin-bottom: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
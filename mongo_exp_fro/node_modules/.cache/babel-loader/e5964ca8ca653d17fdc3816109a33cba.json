{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"ingredientManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {},\n      //某行数据\n      rules: {\n        change_reserves: [{\n          pattern: /^[1-9]\\d*\\.\\d*|0\\.\\d*[1-9]\\d*|0?\\.0+|0$/,\n          require: true,\n          message: \"不能为负数\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    save() {\n      request.post(\"/\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    load() {\n      request.get(\"/ingredient/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwDA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,WAAW,EAAE,CAFR;MAGLC,QAAQ,EAAE,EAHL;MAILC,KAAK,EAAE,CAJF;MAKLC,SAAS,EAAE,EALN;MAMLC,IAAI,EAAE,EAND;MAMK;MACVC,KAAK,EAAE;QACLC,eAAe,EAAE,CACf;UACEC,OAAO,EAAE,yCADX;UAEEC,OAA
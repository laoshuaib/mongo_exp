{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"courseManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      addDialogVisible: false,\n      buyDialogVisible: false,\n      tableData: [],\n      form: {},\n      //某行数据&&添加数据\n      buyForm: {},\n      //采购数据\n      feeForm: {},\n      //费用数据\n      search: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/course/view\").then(res => {\n        this.tableData = res.data;\n      });\n    },\n\n    find() {\n      request.get(\"/course/find\", {\n        params: {\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      request.post(\"/course/save\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.addDialogVisible = false;\n          this.form = {};\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    confirm() {\n      this.$confirm(\"若食材名被修改,相关配方等内容可能发生变动, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.update();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消修改\"\n        });\n      });\n    },\n\n    buyConfirm() {\n      this.$confirm(\"采购记录一旦产生无法删除, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.saveBuy();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消采购\"\n        });\n      });\n    },\n\n    update() {\n      request.post(\"/ingredient/update\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    saveBuy() {\n      request.post(\"/ingredient/saveBuy\", this.buyForm).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"采购成功\"\n          });\n          this.buyDialogVisible = false;\n          this.load(); //产生费用\n\n          this.saveFee();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    saveFee() {\n      this.feeForm.price = this.buyForm.price;\n      request.post(\"/fee/save\", this.feeForm).then(res => {\n        console.log(res);\n\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"相关费用已自动登记\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    handleBuy(row) {\n      let str = sessionStorage.getItem(\"user\"); //清空采购表单\n\n      this.buyForm.amount = \"\";\n      this.buyForm.price = \"\"; //采购表单默认数据\n\n      this.buyForm.userID = JSON.parse(str).id;\n      this.buyForm.ingredientID = JSON.parse(JSON.stringify(row)).id;\n      this.buyForm.name = JSON.parse(JSON.stringify(row)).name; //费用表单默认数据(不对用户展示)\n\n      this.feeForm.type = \"采购\";\n      this.feeForm.userID = JSON.parse(str).id;\n      this.feeForm.note = \"采购\" + JSON.parse(JSON.stringify(row)).name; //弹框控制\n\n      this.buyDialogVisible = true;\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkIA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,gBAAgB,EAAE,KAFb;MAGLC,gBAAgB,EAAE,KAHb;MAILC,SAAS,EAAE,EAJN;MAKLC,IAAI,EAAE,EALD;MAKK;MACVC,OAAO,EAAE,EANJ;MAMQ;MACbC,OAAO,EAAE,EAPJ;MAOQ;MACbC,MAAM,EAAE;IARH,CAAP;EAUD,CAdY;;EAebC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAjBY;;EAkBbC,OAAO,EAAE;IACPD,IAAI,GAAG;MACLZ,OAAO,CAACc,GAAR,CAAY,cAAZ,EAA4BC,IAA5B,CAAkCC,GAAD,IAAS;QACxC,KAAKV,SAAL,GAAiBU,GAAG,CAACd,IAArB;MACD,CAFD;IAGD,CALM;;IAMPe,IAAI,GAAG;MACLjB,OAAM,CACHc,GADH,CACO,cADP,EACuB;QACnBI,MAAM,EAAE;UACNR,MAAM,EAAE,KAAKA;QADP;MADW,CADvB,EAMGK,IANH,CAMSC,GAAD,IAAS;QACb,KAAKV,SAAL,GAAiBU,GAAG,CAACd,IAArB;QACA,KAAKiB,KAAL,GAAaH,GAAG,CAACd,IAAJ,CAASiB,KAAtB;MACD,CATH;IAUD,CAjBM;;IAkBPC,IAAI,GAAG;MACLpB,OAAO,CAACqB,IAAR,CAAa,cAAb,EAA6B,KAAKd,IAAlC,EAAwCQ,IAAxC,CAA8CC,GAAD,IAAS;QACpD,IAAIA,GAAG,CAACM,IAAJ,KAAa,CAAjB,EAAoB;UAClB,KAAKC,QAAL,CAAc;YACZC,IAAI,EAAE,SADM;YAEZC,OAAO,EAAE;UAFG,CAAd;UAIA,KAAKrB,gBAAL,GAAwB,KAAxB;UACA,KAAKG,IAAL,GAAY,EAAZ;UACA,KAAKK,IAAL;QACF,CARA,MAQO;UACL,KAAKW,QAAL,CAAc;YACZC,IAAI,EAAE,OADM;YAEZC,OAAO,EAAET,GAAG,CAACU;UAFD,CAAd;QAIF;MACD,CAfD;IAgBD,CAnCM;;IAoCPC,OAAO,GAAG;MACR,KAAKC,QAAL,CACE,8BADF,EAEE,IAFF,EAGE;QACEC,iBAAiB,EAAE,IADrB;QAEEC,gBAAgB,EAAE,IAFpB;QAGEN,IAAI,EAAE;MAHR,CAHF,EASGT,IATH,CASQ,MAAM;QACV,KAAKgB,MAAL;MACD,CAXH,EAYGC,KAZH,CAYS,MAAM;QACX,KAAKT,QAAL,CAAc;UACZC,IAAI,EAAE,MADM;UAEZC,OAAO,EAAE;QAFG,CAAd;MAID,CAjBH;IAkBD,CAvDM;;IAwDPQ,UAAU,GAAG;MACX,KAAKL,QAAL,CAAc,qBAAd,EAAqC,IAArC,EAA2C;QACzCC,iBAAiB,EAAE,IADsB;QAEzCC,gBAAgB,EAAE,IAFuB;QAGzCN,IAAI,EAAE;MAHmC,CAA3C,EAKGT,IALH,CAKQ,MAAM;QACV,KAAKmB,OAAL;MACD,CAPH,EAQGF,KARH,CAQS,MAAM;QACX,KAAKT,QAAL,CAAc;UACZC,IAAI,EAAE,MADM;UAEZC,OAAO,EAAE;QAFG,CAAd;MAID,CAbH;IAcD,CAvEM;;IAwEPM,MAAM,GAAG;MACP/B,OAAO,CAACqB,IAAR,CAAa,oBAAb,EAAmC,KAAKd,IAAxC,EAA8CQ,IAA9C,CAAoDC,GAAD,IAAS;QAC1DmB,OAAO,CAACC,GAAR
{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"cuisineManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      drawerVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      usingData: [],\n      //配方显示\n      form: {},\n      //某行数据\n      search: \"\",\n      cuisineID: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    addCuisinePush() {\n      this.$router.push(\"/manager/addCuisine\");\n    },\n\n    find() {\n      request.get(\"/cuisine/find\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    deleteConfirm(row) {\n      this.$confirm(\"菜品删除不可撤销, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.delete(row);\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n\n    confirm() {\n      this.$confirm(\"若菜品名被修改,相关订单等内容可能发生变动, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.update();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消修改\"\n        });\n      });\n    },\n\n    openDrawer(row) {\n      this.drawerVisible = true;\n      this.cuisineID = row.id.toString();\n      request.post(\"/cuisine/viewUsingList\", this.cuisineID).then(res => {\n        this.usingData = res.data;\n      });\n    },\n\n    update() {\n      request.post(\"/cuisine/update\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    delete(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      request.post(\"/cuisine/delete\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            showClose: true,\n            type: \"success\",\n            message: \"已删除\"\n          });\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    load() {\n      request.get(\"/cuisine/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyGA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,eADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,aAAa,EAAE,KAFV;MAGLC,WAAW,EAAE,CAHR;MAILC,QAAQ,EAAE,EAJL;MAKLC,KAAK,EAAE,CALF;MAMLC,SAAS,EAAE,EANN;MAOLC,SAAS,EAAE,EAPN;MAOU;MACfC,IAAI,EAAE,EARD;MAQK;MACVC,MAAM,EAAE,EATH;MAULC,SAAS,EAAE;IAVN,CAAP;EAYD,CAhBY;;EAiBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAnBY;;EAoBbC,OAAO,EAAE;IACPC,cAAc,GAAG;MACf,KAAKC,OAAL,CAAaC,IAAb,CAAkB,qBAAlB;IACD,CAHM;;IAIPC,IAAI,GAAG;MACLnB,OAAM,CACHoB,GADH,CACO,eADP,EACwB;QACpBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKjB,WADR;UAENC,QAAQ,EAAE,KAAKA,
        <el-form-item label=\"菜品名\">\n          <el-input v-model=\"form.name\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"价格(元)\">\n          <el-input\n            v-model=\"form.price\"\n            oninput=\"value=value.replace(/[^0-9.]/g,'')\"\n          ></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n        <span>\n          <el-button type=\"primary\" color=\"#fab6b6\" @click=\"confirm\"\n            >保存</el-button\n          >\n        </span>\n      </template>\n    </el-dialog>\n    <el-table :data=\"tableData\" border=\"border\">\n      <el-table-column prop=\"id\" label=\"菜品ID\" width=\"120px\" />\n      <el-table-column prop=\"name\" label=\"菜品名\" />\n      <el-table-column prop=\"price\" label=\"价格(元)\" />\n      <el-table-column label=\"操作\" width=\"200px\" align=\"center\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-button\n            size=\"mini\"\n            @click=\"deleteConfirm(scope.row)\"\n            color=\"#fab6b6\"\n            >删除</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n        v-model:currentPage=\"currentPage\"\n        :page-sizes=\"[5, 10]\"\n        :page-size=\"pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"../../untils/request\";\n\nexport default {\n  name: \"cuisineManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {}, //某行数据\n      search: \"\",\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    addCuisinePush() {\n      this.$router.push(\"/manager/addCuisine\");\n    },\n    find() {\n      request\n        .get(\"/cuisine/find\", {\n          params: {\n            pageNum: this.currentPage,\n            pageSize: this.pageSize,\n            search: this.search,\n          },\n        })\n        .then((res) => {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        });\n    },\n    confirm() {\n      this.$confirm(\n        \"若食材名被修改,相关配方等内容可能发生变动, 是否继续?\",\n        \"提示\",\n        {\n          confirmButtonText: \"确定\",\n          cancelButtonText: \"取消\",\n          type: \"warning\",\n        }\n      )\n        .then(() => {\n          this.update();\n        })\n        .catch(() => {\n          this.$message({\n            type: \"info\",\n            message: \"已取消修改\",\n          });\n        });\n    },\n    update() {\n      request.post(\"/ingredient/update\", this.form).then((res) => {\n        console.log(res);\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\",\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg,\n          });\n        }\n      });\n    },\n    load() {\n      request\n        .get(\"/cuisine/view\", {\n          params: {\n            pageNum: this.currentPage,\n            pageSize: this.pageSize,\n          },\n        })\n        .then((res) => {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        });\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n/deep/ .el-input {\n  width: 85%;\n}\n\n/deep/ .el-dialog {\n  background: #fcf9f4;\n}\n\n/deep/.el-form-item {\n  margin-bottom: 30px;\n}\n</style>\n<style lang=\"less\">\n.el-message-box {\n  background: #fcf9f4;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
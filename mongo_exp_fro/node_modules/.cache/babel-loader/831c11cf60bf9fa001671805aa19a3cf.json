{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"cuisineManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      form: {},\n      //某行数据\n      search: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    addCuisinePush() {\n      this.$router.push(\"/manager/addCuisine\");\n    },\n\n    find() {\n      request.get(\"/cuisine/find\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    confirm() {\n      this.$confirm(\"若食材名被修改,相关配方等内容可能发生变动, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.update();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消修改\"\n        });\n      });\n    },\n\n    update() {\n      request.post(\"/ingredient/update\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    load() {\n      request.get(\"/cuisine/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0FA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,eADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,WAAW,EAAE,CAFR;MAGLC,QAAQ,EAAE,EAHL;MAILC,KAAK,EAAE,CAJF;MAKLC,SAAS,EAAE,EALN;MAMLC,IAAI,EAAE,EAND;MAMK;MACVC,MAAM,EAAE;IAPH,CAAP;EASD,CAbY;;EAcbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAhBY;;EAiBbC,OAAO,EAAE;IACPC,cAAc,GAAG;MACf,KAAKC,OAAL,CAAaC,IAAb,CAAkB,qBAAlB;IACD,CAHM;;IAIPC,IAAI,GAAG;MACLhB,OAAM,CACHiB,GADH,CACO,eADP,EACwB;QACpBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKf,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNI,MAAM,EAAE,KAAKA;QAHP;MADY,CADxB,EAQGW,IARH,CAQSC,GAAD,IAAS;QACb,KAAKd,SAAL,GAAiBc,GAAG,CAACnB,IAAJ,CAASoB,OAA1B;QACA,KAAKhB,KAAL,GAAae,GAAG,CAACnB,IAAJ,CAASI,KAAtB;MACD,CAXH;IAYD,CAjBM;;IAkBPiB,OAAO,GAAG;MACR,KAAKC,QAAL,CACE,8BADF,EAEE,IAFF,EAGE;QACEC,iBAAiB,EAAE,IADrB;QAEEC,gBAAgB,EAAE,IAFpB;QAGEC,IAAI,EAAE;MAHR,CAHF,EASGP,IATH,CASQ,MAAM;QACV,KAAKQ,MAAL;MACD,CAXH,EAYGC,KAZH,CAYS,MAAM;QACX,KAAKC,QAAL,CAAc;UACZH,IAAI,EAAE,MADM;UAEZI,OAAO,EAAE;QAFG,CAAd;MAID,CAjBH;IAkBD,CArCM;;IAsCPH,MAAM,GAAG;MACP5B,OAAO,CAACgC,IAAR,CAAa,oBAAb,EAAmC,KAAKxB,IAAxC,EAA8CY,IAA9C,CAAoDC,GAAD,IAAS;QAC1DY,OAAO,CAACC,GAAR,CAAYb,GAAZ;;QACA,IAAIA,GAAG,CAACc,IAAJ,KAAa,CAAjB,EAAoB;UAClB,KAAKL,QAAL,CAAc;YACZH,IAAI,EAAE,SADM;YAEZI,OAAO,EAAE;UAFG,CAAd;UAIA,KAAK5B,aAAL,GAAqB,KAArB;UACA,KAAKQ,IAAL;QACF,CAPA,MAOO;UACL,KAAKmB,QAAL,CAAc;YACZH,IAAI,EAAE,OADM;YAEZI,OAAO,EAAEV,GAAG,CAACe;UAFD,CAAd;QAIF;MACD,CAfD;IAgBD,CAvDM;;IAwDPzB,IAAI,GAAG;MACLX,OAAM,CACHiB,GADH,CACO,eADP,EACwB;QACpBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKf,WADR;UAENC,QAAQ,EAAE,KAAKA;QAFT;MADY,CADxB,EAOGe,IAPH,CAOSC,GAAD,IAAS;QACb,KAAKd,SAAL,GAAiBc,GAAG,CAACnB,IAAJ,CAASoB,OAA1B;QACA,KAAKhB,KAAL,GAAae,GAAG,CAACnB,IAAJ,CAASI,KAAtB;MACD,CAVH;IAWD,CApEM;;IAqEP
{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"courseManage\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      addDialogVisible: false,\n      uploadDialogVisible: false,\n      buyDialogVisible: false,\n      tableData: [],\n      form: {},\n      //某行数据&&添加数据\n      buyForm: {},\n      //采购数据\n      feeForm: {},\n      //费用数据\n      search: \"\",\n      extraData: {} //用于上传附带参数\n\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/course/view\").then(res => {\n        this.tableData = res.data;\n      });\n    },\n\n    find() {\n      request.get(\"/course/find\", {\n        params: {\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      request.post(\"/course/save\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.addDialogVisible = false;\n          this.form = {};\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    handleSuccess(res) {\n      if (res.code === 0) {\n        this.$message({\n          type: \"success\",\n          message: \"上传成功\"\n        });\n        this.load();\n      } else {\n        this.$message({\n          type: \"error\",\n          message: res.msg\n        });\n      }\n    },\n\n    handleError() {\n      this.$message({\n        type: \"error\",\n        message: \"excel数据格式错误\"\n      });\n      this.load();\n    },\n\n    confirm() {\n      this.$confirm(\"若食材名被修改,相关配方等内容可能发生变动, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.update();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消修改\"\n        });\n      });\n    },\n\n    buyConfirm() {\n      this.$confirm(\"采购记录一旦产生无法删除, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.saveBuy();\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消采购\"\n        });\n      });\n    },\n\n    update() {\n      request.post(\"/ingredient/update\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    saveBuy() {\n      request.post(\"/ingredient/saveBuy\", this.buyForm).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"采购成功\"\n          });\n          this.buyDialogVisible = false;\n          this.load(); //产生费用\n\n          this.saveFee();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    saveFee() {\n      this.feeForm.price = this.buyForm.price;\n      request.post(\"/fee/save\", this.feeForm).then(res => {\n        console.log(res);\n\n        if (res.code === 0) {\n          this.$message({\n            type: \"success\",\n            message: \"相关费用已自动登记\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    handleBuy(row) {\n      let str = sessionStorage.getItem(\"user\"); //清空采购表单\n\n      this.buyForm.amount = \"\";\n      this.buyForm.price = \"\"; //采购表单默认数据\n\n      this.buyForm.userID = JSON.parse(str).id;\n      this.buyForm.ingredientID = JSON.parse(JSON.stringify(row)).id;\n      this.buyForm.name = JSON.parse(JSON.stringify(row)).name; //费用表单默认数据(不对用户展示)\n\n      this.feeForm.type = \"采购\";\n      this.feeForm.userID = JSON.parse(str).id;\n      this.feeForm.note = \"采购\" + JSON.parse(JSON.stringify(row)).name; //弹框控制\n\n      this.buyDialogVisible = true;\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwJA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,gBAAgB,EAAE,KAFb;MAGLC,mBAAmB,EAAE,KAHhB;MAILC,gBAAgB,EAAE,KAJb;MAKLC,SAAS,EAAE,EALN;MAMLC,IAAI,EAAE,EAND;MAMK;MACVC,OAAO,EAAE,EAPJ;MAOQ;MACbC,OAAO,EAAE,EARJ;MAQQ;MACbC,MAAM,EAAE,EATH;MAULC,SAAS,EAAE,EAVN,CAUU;;IAVV,CAAP;EAYD,CAhBY;;EAiBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAnBY;;EAoBbC,OAAO,EAAE;IACPD,IAAI,GAAG;MACLd,OAAO,CAACgB,GAAR,CAAY,cAAZ,EAA4BC,IAA5B,CAAkCC,GAAD,IAAS;QACxC,KAAKX,SAAL,GAAiBW,GAAG,CAAChB,IAArB;MACD,CAFD;IAGD,CALM;;IAMPiB,IAAI,GAAG;MACLnB,OAAM,CACHgB,GADH,CACO,cADP,EACuB;QACnBI,MAAM,EAAE;UACNT,MAAM,EAAE,KAAKA;QADP;MADW,CADvB,EAMGM,IANH,CAMSC,GAAD,IAAS;QACb,KAAKX,SAAL,GAAiBW,GAAG,CAAChB,IAArB;QACA,KAAKmB,KAAL,GAAaH,GAAG,CAAChB,IAAJ,CAASmB,KAAtB;MACD,CATH;IAUD,CAjBM;;IAkBPC,IAAI,GAAG;MACLtB,OAAO,CAACuB,IAAR,CAAa,cAAb,EAA6B,KAAKf,IAAlC,EAAwCS,IAAxC,CAA8CC,GAAD,IAAS;QACpD,IAAIA,GAAG,CAACM,IAAJ,KAAa,CAAjB,EAAoB;UAClB,KAAKC,QAAL,CAAc;YACZC,IAAI,EAAE,SADM;YAEZC,OAAO,EAAE;UAFG,CAAd;UAIA,KAAKvB,gBAAL,GAAwB,KAAxB;UACA,KAAKI,IAAL,GAAY,EAAZ;UACA,KAAKM,IAAL;QACF,CARA,MAQO;UACL,KAAKW,QAAL,CAAc;YACZC,IAAI,EAAE,OADM;YAEZC,OAAO,EAAET,GAAG,CAACU;UAFD,CAAd;QAIF;MACD,CAfD;IAgBD,CAnCM;;IAoCPC,aAAa,CAACX,GAAD,EAAM;MACjB,IAAIA,GAAG,CAACM,IAAJ,KAAa,CAAjB,EAAoB;QAClB,KAAKC,QAAL,CAAc;UACZC,IAAI,EAAE,SADM;UAEZC,OAAO,EAAE;QAF
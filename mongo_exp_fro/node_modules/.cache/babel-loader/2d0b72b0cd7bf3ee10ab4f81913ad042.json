{"ast":null,"code":"import request from \"../../untils/request\";\nexport default {\n  name: \"orderManage\",\n\n  data() {\n    return {\n      drawerVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      usingData: [],\n      //订单内容显示\n      form: {},\n      //某行数据\n      search: \"\",\n      ordersID: \"\"\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    format(row) {\n      return row.type === 0 ? \"堂食\" : row.type === 1 ? \"外卖\" : \"未填写\";\n    },\n\n    find() {\n      request.get(\"/orders/find\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    deleteConfirm(row) {\n      this.$confirm(\"菜品删除不可撤销, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.delete(row);\n      }).catch(() => {\n        this.$message({\n          type: \"info\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n\n    openDrawer(row) {\n      this.drawerVisible = true;\n      this.ordersID = row.id.toString();\n      request.get(\"/orders/viewContainerList\", {\n        params: {\n          ordersID: this.ordersID\n        }\n      }).then(res => {\n        this.usingData = res.data;\n      });\n    },\n\n    delete(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      request.post(\"/orders/delete\", this.form).then(res => {\n        if (res.code === 0) {\n          this.$message({\n            showClose: true,\n            type: \"success\",\n            message: \"已删除\"\n          });\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    load() {\n      request.get(\"/orders/view\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA,OAAOA,OAAP,MAAoB,sBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,WAAW,EAAE,CAFR;MAGLC,QAAQ,EAAE,EAHL;MAILC,KAAK,EAAE,CAJF;MAKLC,SAAS,EAAE,EALN;MAMLC,SAAS,EAAE,EANN;MAMU;MACfC,IAAI,EAAE,EAPD;MAOK;MACVC,MAAM,EAAE,EARH;MASLC,QAAQ,EAAE;IATL,CAAP;EAWD,CAfY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,MAAM,CAACC,GAAD,EAAM;MACV,OAAOA,GAAG,CAACC,IAAJ,KAAa,CAAb,GAAiB,IAAjB,GAAwBD,GAAG,CAACC,IAAJ,KAAa,CAAb,GAAiB,IAAjB,GAAwB,KAAvD;IACD,CAHM;;IAIPC,IAAI,GAAG;MACLlB,OAAM,CACHmB,GADH,CACO,cADP,EACuB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKjB,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNK,MAAM,EAAE,KAAKA;QAHP;MADW,CADvB,EAQGY,IARH,CAQSC,GAAD,IAAS;QACb,KAAKhB,SAAL,GAAiBgB,GAAG,CAACrB,IAAJ,CAASsB,OAA1B;QACA,KAAKlB,KAAL,GAAaiB,GAAG,CAACrB,IAAJ,CAASI,KAAtB;MACD,CAXH;IAYD,CAjBM;;IAkBPmB,aAAa,CAACT,GAAD,EAAM;MACjB,KAAKU,QAAL,CAAc,iBAAd,EAAiC,IAAjC,EAAuC;QACrCC,iBAAiB,EAAE,IADkB;QAErCC,gBAAgB,EAAE,IAFmB;QAGrCX,IAAI,EAAE;MAH+B,CAAvC,EAKGK,IALH,CAKQ,MAAM;QACV,KAAKO,MAAL,CAAYb,GAAZ;MACD,CAPH,EAQGc,KARH,CAQS,MAAM;QACX,KAAKC,QAAL,CAAc;UACZd,IAAI,EAAE,MADM;UAEZe,OAAO,EAAE;QAFG,CAAd;MAID,CAbH;IAcD,CAjCM;;IAkCPC,UAAU,CAACjB,GAAD,EAAM;MACd,KAAKb,aAAL,GAAqB,IAArB;MACA,KAAKQ,QAAL,GAAgBK,GAAG,CAACkB,EAAJ,CAAOC,QAAP,EAAhB;MACAnC,OAAM,CACHmB,GADH,CACO,2BADP,EACoC;QAChCC,MAAM,EAAE;UAAET,QAAQ,EAAE,KAAKA;QAAjB;MADwB,CADpC,EAIGW,IAJH,CAISC,GAAD,IAAS;QACb,KAAKf,SAAL,GAAiBe,GAAG,CAACrB,IAArB;MACD,CANH;IAO